  "description": "A simple API for managing clients using Elysia and Prisma",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "prisma": "^5.21.1"
  },
  "repository": {
    "type": "git",
