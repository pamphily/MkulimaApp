  },
  "homepage": "https://github.com/lakwetu-tz/sts-client-api#readme",
  "bugs": {
    "url": "https://github.com/lakwetu-tz/sts-client-api/issues"
  },
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "bun --watch run server/index.ts",
